<!-- 검색엔진 최적화 메터태그
메타태그는 검색엔진 SW에 현재 페이지가 무슨정보를 제공하는지를 알려주는 역할을 제공해서
검색사이트에 우리가 제공하는 웹페이지 정보를 반영해주는 기능제공태그  -->
<meta name="description" content="게시글 정보 등록 웹페이지" />

<style></style>

<!--우측 콘텐츠 헤더영역 -->
<div class="row wrapper border-bottom white-bg page-heading">
  <div class="col-lg-10">
    <h2>학생정보 관리</h2>
    <ol class="breadcrumb">
      <li><a href="/Home/Index">Home</a>></li>
      <li><a>학생생정보관리</a>></li>
      <li class="active">
        <strong>신규학생추가</strong>
      </li>
    </ol>
  </div>
  <div class="col-lg-2"></div>
</div>

<!--우측 콘텐츠 영역 -->
<div class="wrapper wrapper-content">
  <form id="createForm" action="/article/create" method="post">
    <div class="ibox-content m-b-sm border-bottom">
      <!-- 학생이름영역 -->
      <div class="row">
        <div class="col-sm-12">
          <div class="form-group">
            <label class="control-label">이름</label>
            <input type="text" id="name" name="name" class="form-control" />
          </div>
        </div>
      </div>

      <!-- 학번영역 -->
      <div class="row">
        <div class="col-sm-12">
          <div class="form-group">
            <label class="control-label">학번</label>
            <input
              type="int"
              id="student_id"
              name="student_id"
              class="form-control"
            />
          </div>
        </div>
      </div>

      <!-- 성별영역 -->
      <div class="row">
        <div class="col-sm-12">
          <div class="form-group">
            <label class="control-label">성별</label>
            <input type="text" id="gender" name="gender" class="form-control" />
          </div>
        </div>
      </div>

      <!-- 전공영역 -->
      <div class="row">
        <div class="col-sm-12">
          <div class="form-group">
            <label class="control-label">전공</label>
            <input type="text" id="major" name="major" class="form-control" />
          </div>
        </div>
      </div>

      <!-- 생년월일영역 -->
      <div class="row">
        <div class="col-sm-12">
          <div class="form-group">
            <label class="control-label">생년월일</label>
            <input type="date" id="birth" name="birth" class="form-control" />
          </div>
        </div>
      </div>

      <!-- 핸드폰번호영역 -->
      <div class="row">
        <div class="col-sm-12">
          <div class="form-group">
            <label class="control-label">핸드폰번호</label>
            <input type="text" id="phone" name="phone" class="form-control" />
          </div>
        </div>
      </div>

      <!-- 동아리번호영역 -->
      <div class="row">
        <div class="col-sm-12">
          <div class="form-group">
            <label class="control-label">동아리번호</label>
            <input
              type="int"
              id="club_id"
              name="club_id"
              class="form-control"
            />
          </div>
        </div>
      </div>

      <div class="text-center">
        <button type="submit" class="btn btn-primary">저장</button>
        <a href="/article/list" class="btn btn-info">목록</a>
      </div>
    </div>
  </form>
</div>

<script></script>
